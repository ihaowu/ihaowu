    "dev": "tsc -b -w",
    "build": "nest build",
    "lint": "eslint --fix \"**/*.{ts,js}\"",
    "format": "prettier --write \"**/*.{ts,js,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
