{
  "collection": "@nestjs/schematics",
  "monorepo": true,
  "root": "apps/api",
  "sourceRoot": "apps/api/src",
  "compilerOptions": {
    "webpack": false,
    "tsConfigPath": "apps/api/tsconfig.json"
  },
  "projects": {
    "api": {
      "type": "application",
      "root": "apps/api",
      "entryFile": "main",
      "sourceRoot": "apps/api/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api/tsconfig.json"
      }
    },
    "passport": {
      "type": "application",
      "root": "apps/passport",
      "entryFile": "main",
      "sourceRoot": "apps/passport/src",
      "compilerOptions": {
        "tsConfigPath": "apps/passport/tsconfig.json"
      }
    },
    "config": {
      "type": "application",
      "root": "microservices/config",
      "entryFile": "main",
      "sourceRoot": "microservices/config/src",
      "compilerOptions": {
        "tsConfigPath": "microservices/config/tsconfig.json"
      }
    },
    "wechat": {
      "type": "application",
      "root": "microservices/wechat",
      "entryFile": "main",
      "sourceRoot": "microservices/wechat/src",
      "compilerOptions": {
        "tsConfigPath": "microservices/wechat/tsconfig.json"
      }
    }
  }
}
